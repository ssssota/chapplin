		"oxc-parser": "catalog:",
		"preact": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"tsdown": "^0.17.0",
		"typescript": "catalog:",
		"vite": "catalog:",
		"vite-plugin-singlefile": "catalog:",
		"zod": "catalog:",
		"zimmerframe": "catalog:"
	},
	"dependencies": {
		"magic-string": "catalog:",
		"oxc-parser": "catalog:",
		"vite-plugin-singlefile": "catalog:",
