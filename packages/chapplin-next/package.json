{
	"name": "chapplin-next",
	"type": "module",
	"version": "0.0.1",
	"description": "All-in-one framework for building MCP Server with MCP Apps",
	"scripts": {
		"build": "tsdown",
		"check": "tsc -b --noEmit",
		"dev": "tsdown --watch"
