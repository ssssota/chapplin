		"vite": ">=6",
		"zod": ">=3"
	},
	"peerDependenciesMeta": {
		"@modelcontextprotocol/ext-apps": {
			"optional": true
		},
		"hono": {
			"optional": true
		},
		"react": {
			"optional": true
		},
		"react-dom": {
			"optional": true
		},
		"preact": {
			"optional": true
		},
		"solid-js": {
			"optional": true
		}
	},
	"devDependencies": {
		"@modelcontextprotocol/ext-apps": "^1.0.1",
		"@modelcontextprotocol/sdk": "catalog:",
