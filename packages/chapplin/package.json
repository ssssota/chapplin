{
	"name": "chapplin",
	"type": "module",
	"version": "0.0.0",
	"scripts": {
		"build": "tsc",
		"check": "tsc --noEmit"
	},
	"bin": {
		"chapplin": "./dist/cli.js"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./tool": {
			"types": "./dist/tool.d.ts",
			"preact": "./dist/tool-preact.js",
			"hono": "./dist/tool-hono.js",
			"browser": "./dist/tool-react.js",
			"default": "./dist/tool.js"
		}
	},
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ssssota/chapplin.git"
	},
	"license": "MIT",
	"peerDependencies": {
		"@modelcontextprotocol/sdk": ">=1",
		"zod": "^3",
		"hono": ">=4",
		"react": ">=18",
		"react-dom": ">=18",
		"preact": ">=10"
	},
	"peerDependenciesMeta": {
		"hono": {
			"optional": true
		},
		"react": {
			"optional": true
		},
		"react-dom": {
			"optional": true
		},
		"preact": {
			"optional": true
		}
	},
	"devDependencies": {
		"@modelcontextprotocol/sdk": "catalog:",
		"@types/node": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"hono": "catalog:",
		"preact": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"typescript": "catalog:",
		"zod": "catalog:"
	},
	"dependencies": {
		"rolldown": "catalog:"
	}
}
