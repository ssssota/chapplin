{
	"name": "chapplin",
	"type": "module",
	"version": "1.2.0",
	"description": "All-in-one framework for building MCP Server with MCP Apps",
	"scripts": {
		"build": "tsdown && pnpm build:dev-ui",
		"build:dev-ui": "vite build dev-ui",
		"check": "tsc -b --noEmit",
		"dev": "tsdown --watch",
		"prepare": "pnpm build",
		"test": "vitest"
	},
	"bin": {
		"chapplin": "./bin/index.mjs"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./vite": {
			"types": "./dist/vite/index.d.ts",
			"default": "./dist/vite/index.js"
		},
		"./react": {
			"types": "./dist/react.d.ts",
			"default": "./dist/react.js"
		},
		"./preact": {
			"types": "./dist/preact.d.ts",
			"default": "./dist/preact.js"
		},
		"./solid": {
			"types": "./dist/solid.d.ts",
			"default": "./dist/solid.js"
		},
		"./hono": {
			"types": "./dist/hono.d.ts",
			"default": "./dist/hono.js"
		},
		"./client/react": "./dist/client/react.js",
		"./client/preact": "./dist/client/preact.js",
		"./client/solid": "./dist/client/solid.js",
		"./client/hono": "./dist/client/hono.js"
	},
	"files": [
		"dist",
		"bin"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ssssota/chapplin.git",
		"directory": "packages/chapplin"
	},
	"license": "MIT",
	"peerDependencies": {
		"@modelcontextprotocol/ext-apps": ">=1.0.0",
		"@modelcontextprotocol/sdk": ">=1.22",
		"hono": ">=4.0.0",
		"preact": ">=10.0.0",
		"react": ">=18.0.0",
		"react-dom": ">=18.0.0",
		"solid-js": ">=1.0.0",
		"vite": ">=6",
		"zod": ">=3"
	},
	"peerDependenciesMeta": {
		"@modelcontextprotocol/ext-apps": {
			"optional": true
		},
		"hono": {
			"optional": true
		},
		"preact": {
			"optional": true
		},
		"react": {
			"optional": true
		},
		"react-dom": {
			"optional": true
		},
		"solid-js": {
			"optional": true
		}
	},
	"devDependencies": {
		"@modelcontextprotocol/ext-apps": "catalog:",
		"@modelcontextprotocol/sdk": "catalog:",
		"@preact/preset-vite": "^2.10.3",
		"@types/node": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"@unocss/reset": "catalog:",
		"hono": "catalog:",
		"preact": "catalog:",
		"preact-iso": "^2.11.1",
		"react": "catalog:",
		"react-dom": "catalog:",
		"solid-js": "catalog:",
		"tsdown": "^0.20.3",
		"typescript": "catalog:",
		"unocss": "catalog:",
		"vite": "catalog:",
		"vite-plugin-dev-api": "^0.2.1",
		"vite-plugin-singlefile": "catalog:",
		"vitest": "^4.0.18",
		"zod": "catalog:"
	},
	"dependencies": {
		"vite-plugin-dev-api": "^0.2.1",
		"vite-plugin-singlefile": "catalog:"
	}
}
