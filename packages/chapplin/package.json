{
	"name": "chapplin",
	"type": "module",
	"version": "0.0.1",
	"description": "All-in-one framework for building MCP Server with MCP Apps",
	"scripts": {
		"build": "tsdown && pnpm build:dev-ui",
		"build:dev-ui": "vite build dev-ui",
		"check": "tsc -b --noEmit",
		"dev": "tsdown --watch",
		"prepare": "pnpm build",
		"test": "vitest"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./vite": {
			"types": "./dist/vite/index.d.ts",
			"default": "./dist/vite/index.js"
		},
		"./client/react": {
			"types": "./dist/client/react.d.ts",
			"default": "./dist/client/react.js"
		},
		"./client/preact": {
			"types": "./dist/client/preact.d.ts",
			"default": "./dist/client/preact.js"
		},
		"./client/solid": {
			"types": "./dist/client/solid.d.ts",
			"default": "./dist/client/solid.js"
		},
		"./client/hono": {
			"types": "./dist/client/hono.d.ts",
			"default": "./dist/client/hono.js"
		}
	},
	"files": [
		"dist",
		"iframe.html"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ssssota/chapplin.git",
		"directory": "packages/chapplin-next"
	},
	"license": "MIT",
	"peerDependencies": {
		"@modelcontextprotocol/ext-apps": ">=1.0.0",
		"@modelcontextprotocol/sdk": ">=1.22",
		"hono": ">=4.0.0",
		"preact": ">=10.0.0",
		"react": ">=18.0.0",
		"react-dom": ">=18.0.0",
		"solid-js": ">=1.0.0",
		"vite": ">=6",
		"zod": ">=3"
	},
	"peerDependenciesMeta": {
		"@modelcontextprotocol/ext-apps": {
			"optional": true
		},
		"hono": {
			"optional": true
		},
		"preact": {
			"optional": true
		},
		"react": {
			"optional": true
		},
		"react-dom": {
			"optional": true
		},
		"solid-js": {
			"optional": true
		}
	},
	"devDependencies": {
		"@modelcontextprotocol/ext-apps": "^1.0.1",
		"@modelcontextprotocol/sdk": "catalog:",
		"@preact/preset-vite": "^2.9.0",
		"@types/node": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"@unocss/reset": "catalog:",
		"hono": "catalog:",
		"magic-string": "catalog:",
		"preact": "catalog:",
		"preact-iso": "^2.0.0",
		"react": "catalog:",
		"react-dom": "catalog:",
		"tsdown": "^0.17.0",
		"typescript": "catalog:",
		"unocss": "catalog:",
		"vite": "catalog:",
		"vite-plugin-dev-api": "^0.1.0",
		"vite-plugin-singlefile": "catalog:",
		"vitest": "^4.0.15",
		"zod": "catalog:"
	},
	"dependencies": {
		"magic-string": "catalog:",
		"vite-plugin-dev-api": "^0.2.1",
		"vite-plugin-singlefile": "catalog:"
	}
}
