{
	"name": "chapplin",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"build": "tsc",
		"check": "tsc --noEmit",
		"prepare": "pnpm build"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./tool": {
			"types": "./dist/tool.d.ts",
			"default": "./dist/tool.js"
		},
		"./tool-react": "./dist/tool-react.js",
		"./tool-preact": "./dist/tool-preact.js",
		"./tool-hono": "./dist/tool-hono.js",
		"./vite": {
			"types": "./dist/vite/index.d.ts",
			"default": "./dist/vite/index.js"
		}
	},
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ssssota/chapplin.git"
	},
	"license": "MIT",
	"peerDependencies": {
		"@modelcontextprotocol/sdk": ">=1",
		"hono": ">=4",
		"preact": ">=10",
		"react": ">=18",
		"react-dom": ">=18",
		"vite": ">=6",
		"zod": "^3"
	},
	"peerDependenciesMeta": {
		"hono": {
			"optional": true
		},
		"react": {
			"optional": true
		},
		"react-dom": {
			"optional": true
		},
		"preact": {
			"optional": true
		}
	},
	"devDependencies": {
		"@modelcontextprotocol/sdk": "catalog:",
		"@types/node": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"hono": "catalog:",
		"preact": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"typescript": "catalog:",
		"vite": "^7.2.4",
		"zod": "catalog:"
	},
	"dependencies": {
		"@babel/parser": "catalog:",
		"@babel/traverse": "^7.28.5",
		"magic-string": "catalog:",
		"rolldown": "catalog:",
		"vite-plugin-singlefile": "^2.3.0"
	}
}
