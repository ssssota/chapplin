{
	"name": "chapplin",
	"type": "module",
	"version": "0.2.1",
	"scripts": {
		"build": "tsdown && vite build ui",
		"check": "tsc -b --noEmit",
		"prepare": "pnpm build",
		"dev": "vite ui"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./tool": {
			"types": "./dist/tool.d.ts",
			"default": "./dist/tool.js"
		},
		"./_internal/iframe-runtime": "./dist/iframe-runtime.js",
		"./_internal/tool-react": "./dist/tool-react.js",
		"./_internal/tool-preact": "./dist/tool-preact.js",
		"./_internal/tool-hono": "./dist/tool-hono.js",
		"./_internal/tool-solid": "./dist/tool-solid.js",
		"./vite": {
			"types": "./dist/vite/index.d.ts",
			"default": "./dist/vite/index.js"
		}
	},
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ssssota/chapplin.git"
	},
	"license": "MIT",
	"peerDependencies": {
		"@modelcontextprotocol/sdk": ">=1.23",
		"hono": ">=4",
		"preact": ">=10",
		"react": ">=18",
		"react-dom": ">=18",
		"vite": ">=6",
		"zod": ">=4"
	},
	"peerDependenciesMeta": {
		"hono": {
			"optional": true
		},
		"react": {
			"optional": true
		},
		"react-dom": {
			"optional": true
		},
		"preact": {
			"optional": true
		}
	},
	"devDependencies": {
		"@chapplin/internal-types": "file:./types",
		"@modelcontextprotocol/sdk": "catalog:",
		"@preact/preset-vite": "^2.10.2",
		"@preact/signals": "^2.5.1",
		"@types/estree": "^1.0.8",
		"@types/node": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"@unocss/reset": "^66.5.12",
		"hono": "catalog:",
		"preact": "catalog:",
		"preact-iso": "^2.11.1",
		"react": "catalog:",
		"react-dom": "catalog:",
		"solid-js": "^1.9.10",
		"tsdown": "^0.17.0",
		"typescript": "catalog:",
		"unocss": "^66.5.12",
		"valibot": "^1.2.0",
		"vite": "catalog:",
		"zod": "catalog:"
	},
	"dependencies": {
		"magic-string": "catalog:",
		"oxc-parser": "catalog:",
		"vite-plugin-singlefile": "catalog:",
		"zimmerframe": "catalog:"
	}
}
