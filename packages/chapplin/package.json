{
	"name": "chapplin",
	"type": "module",
	"version": "0.1.2",
	"scripts": {
		"build": "tsc",
		"check": "tsc --noEmit",
		"prepare": "pnpm build"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./tool": {
			"types": "./dist/tool.d.ts",
			"default": "./dist/tool.js"
		},
		"./_internal/tool-react": "./dist/tool-react.js",
		"./_internal/tool-preact": "./dist/tool-preact.js",
		"./_internal/tool-hono": "./dist/tool-hono.js",
		"./vite": {
			"types": "./dist/vite/index.d.ts",
			"default": "./dist/vite/index.js"
		}
	},
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ssssota/chapplin.git"
	},
	"license": "MIT",
	"peerDependencies": {
		"@modelcontextprotocol/sdk": ">=1.23",
		"hono": ">=4",
		"preact": ">=10",
		"react": ">=18",
		"react-dom": ">=18",
		"vite": ">=6",
		"zod": ">=4"
	},
	"peerDependenciesMeta": {
		"hono": {
			"optional": true
		},
		"react": {
			"optional": true
		},
		"react-dom": {
			"optional": true
		},
		"preact": {
			"optional": true
		}
	},
	"devDependencies": {
		"@modelcontextprotocol/sdk": "catalog:",
		"@types/estree": "^1.0.8",
		"@types/node": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"hono": "catalog:",
		"preact": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"typescript": "catalog:",
		"vite": "catalog:",
		"zod": "catalog:"
	},
	"dependencies": {
		"magic-string": "catalog:",
		"oxc-parser": "catalog:",
		"vite-plugin-singlefile": "^2.3.0",
		"zimmerframe": "catalog:"
	}
}
